    const form = document.getElementById('surveyForm');
    const formData = new FormData(form);

    // ‡∏Å‡∏≥‡∏´‡∏ô‡∏î‡∏Ñ‡∏π‡πà‡∏Ñ‡∏≥‡∏ñ‡∏≤‡∏°‡∏ó‡∏µ‡πà‡∏ï‡πâ‡∏≠‡∏á‡∏Å‡∏≤‡∏£‡∏£‡∏ß‡∏°‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô
    const pairs = [
        [[1, 2], [13, 14], [25, 26], [37, 38], [49, 50]], // ‡∏´‡∏°‡∏ß‡∏î S
